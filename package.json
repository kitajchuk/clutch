{
  "title": "Clutch",
  "name": "clutch",
  "description": "A modern headless CMS SDK for Prismic and Contentful.",
  "private": true,
  "version": "1.0.4",
  "config": {
    "aws_ec2_dest": "/var/www/html/",
    "aws_ec2_staging_host": "34.209.179.25",
    "aws_ec2_production_host": "put.the.host.here"
  },
  "scripts": {
    "bootstrap": "node clutch.install.js",
    "bootstrap:contentful": "node ./server/adapters/contentful.bootstrap.js",
    "generate:robots": "node ./server/core/robots.js",
    "generate:appcache": "node ./server/generators/appcache.js",
    "start": "echo 'Clutch is starting...' && (npm run server & npm run watch)",
    "watch": "export NODE_ENV=sandbox ; ./node_modules/.bin/webpack --env.sandbox --hide-modules --colors --watch",
    "server": "export NODE_ENV=sandbox ; ./node_modules/.bin/nodemon -i ./static -i ./template -i ./source server/app.js",
    "build": "export NODE_ENV=sandbox ; ./node_modules/.bin/webpack --env.sandbox --progress --profile --display-modules --colors",
    "build:staging": "export NODE_ENV=staging ; ./node_modules/.bin/webpack --env.staging --progress --profile --display-modules --colors -p",
    "build:production": "export NODE_ENV=production ; ./node_modules/.bin/webpack --env.production --progress --profile --display-modules --colors -p",
    "connect:staging": "ssh ec2-user@$npm_package_config_aws_ec2_staging_host -i ~/.ssh/clutch.pem",
    "connect:production": "ssh ec2-user@$npm_package_config_aws_ec2_production_host -i ~/.ssh/clutch.pem",
    "deploy:staging": "npm run build:staging ; npm run generate:robots ; export NODE_ENV=staging ; export AWS_HOST=${AWS_STAGING_HOST} ; ./clutch.deploy",
    "deploy:production": "npm run build:production ; npm run generate:robots ; npm run generate:appcache ; export NODE_ENV=production ; export AWS_HOST=${AWS_PRODUCTION_HOST} ; ./clutch.sync ; ./clutch.deploy",
    "deploy:sandbox:staging": "rsync -av -e 'ssh -i ./sandbox/clutch.pem' --rsync-path 'sudo rsync' --exclude 'clutch.pem' sandbox ec2-user@34.209.179.25:/var/www/html/",
    "deploy:sandbox:production": "mirror sandbox:staging with production EIP here"
  },
  "devDependencies": {
    "appcache-manifest": "^2.1.0",
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "compression-webpack-plugin": "^0.4.0",
    "contentful-management": "^4.2.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "gsap": "^2.0.1",
    "hammerjs": "^2.0.8",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "on-build-webpack": "^0.1.0",
    "paramalama": "~0.2",
    "postcss": "^6.0.22",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.1.0",
    "properjs-controller": "~0.2",
    "properjs-debounce": "^0.2.0",
    "properjs-easing": "~0.2",
    "properjs-hobo": "^0.3.10",
    "properjs-imageloader": ">=0.3.1",
    "properjs-lager": "^0.1.2",
    "properjs-mediabox": "~0.3",
    "properjs-pagecontroller": "~0.4",
    "properjs-resizecontroller": "~0.2",
    "properjs-router": "^0.3.7",
    "properjs-s3": "^0.1.0",
    "properjs-scroll2": "~0.2",
    "properjs-scrollcontroller": "~0.2",
    "properjs-template": "~0.2",
    "properjs-throttle": "^0.3.0",
    "properjs-tween": "~0.2",
    "sass-loader": "^7.0.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4"
  },
  "repository": {
    "type": "git",
