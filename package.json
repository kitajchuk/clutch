{
  "title": "Clutch",
  "name": "clutch",
  "description": "An Open-ended Developer SDK for Prismic.io.",
  "private": true,
  "version": "2.0.0",
  "config": {},
  "scripts": {
    "bootstrap": "node clutch.install.js",
    "bootstrap:hobo": "cd ./node_modules/properjs-hobo && npm install && npm run build -- 'is eq not one next prev attr last first index parent filter detach append remove trigger prepend closest children removeAttr toggleClass'",
    "start": "echo 'Clutch is starting...' && (npm run server & npm run watch)",
    "watch": "export NODE_ENV=sandbox ; ./node_modules/.bin/webpack --env.sandbox --hide-modules --colors --watch",
    "server": "export NODE_ENV=sandbox ; cd ./server && npm run start:sandbox",
    "certbot:development": "export NODE_ENV=development ; node ./server/generators/certbot.auto.js",
    "certbot:production": "export NODE_ENV=production ; node ./server/generators/certbot.auto.js",
    "build": "export NODE_ENV=sandbox ; ./node_modules/.bin/webpack --env.sandbox --progress --profile --display-modules --colors",
    "build:development": "export NODE_ENV=development ; ./node_modules/.bin/webpack --env.development --progress --profile --display-modules --colors -p",
    "build:production": "export NODE_ENV=production ; ./node_modules/.bin/webpack --env.production --progress --profile --display-modules --colors -p",
    "connect:development": "ssh ec2-user@DEVELOPMENT_IP",
    "connect:production": "ssh ec2-user@PRODUCTION_IP",
    "deploy:development": "npm run build:development; export NODE_ENV=development ; export AWS_HOST=${AWS_DEVELOPMENT_HOST} ; ./clutch.deploy",
    "deploy:production": "npm run build:production ; export NODE_ENV=production ; export AWS_HOST=${AWS_PRODUCTION_HOST} ; ./clutch.sync ; ./clutch.deploy",
    "deploy:clutch:development": "rsync -av -e 'ssh' --rsync-path 'sudo rsync' .clutch ec2-user@DEVELOPMENT_IP:/var/www/html/",
    "deploy:clutch:production": "rsync -av -e 'ssh' --rsync-path 'sudo rsync' .clutch ec2-user@PRODUCTION_IP:/var/www/html/"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "gsap": "^2.0.1",
    "hammerjs": "^2.0.8",
    "node-sass": "^4.9.0",
    "on-build-webpack": "^0.1.0",
    "paramalama": "~0.2",
    "postcss": "^6.0.22",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.1.0",
    "properjs-controller": "~0.2",
    "properjs-debounce": "^0.2.0",
    "properjs-easing": "~0.2",
    "properjs-hobo": "^0.3.10",
    "properjs-imageloader": ">=0.3.1",
    "properjs-lager": "^0.1.2",
    "properjs-mediabox": "~0.3",
    "properjs-pagecontroller": "~0.4",
    "properjs-resizecontroller": "~0.2",
    "properjs-router": "^0.3.7",
    "properjs-s3": "^0.1.0",
    "properjs-scroll2": "~0.2",
    "properjs-scrollcontroller": "~0.2",
    "properjs-template": "~0.2",
    "properjs-throttle": "^0.3.0",
    "properjs-tween": "~0.2",
    "sass-loader": "^7.0.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4"
  },
