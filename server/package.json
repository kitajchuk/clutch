{
  "title": "Clutch Server",
  "name": "clutch-server",
  "version": "0.1.0",
  "description": "Express.js app server.",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start:staging": "export NODE_ENV=staging ; ./node_modules/.bin/forever -a -l forever.log -o out.log -e err.log start app.js",
    "start:production": "export NODE_ENV=production ; ./node_modules/.bin/forever -a -l forever.log -o out.log -e err.log start app.js",
    "install:staging": "export NODE_ENV=staging ; node app.install.js",
    "install:production": "export NODE_ENV=production ; node app.install.js",
    "stop": "./node_modules/.bin/forever stopall"
  },
  "contributors": [
    {
      "name": "Brandon Kitajchuk",
      "email": "kitajchuk@gmail.com",
      "url": "http://kitajchuk.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.2",
    "consolidate": "^0.14.1",
    "contentful": "^4.3.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "forever": "^0.15.2",
    "node-watch": "^0.5.3",
    "prismic.io": "^3.5.0",
    "properjs-lager": "^0.1.0"
  }
}
